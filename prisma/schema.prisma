generator client {
  provider        = "prisma-client-js"
  output          = "../generated/client"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
}

model Todo {
  id        Int     @id @default(autoincrement())
  title     String
  content   String
  completed Boolean @default(false)
}
